<h1>Users</h1>
<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th>Id</th>
    <th>Email</th>
    <th>Name</th>
    <th>Username</th>
    <th>Address</th>
    <th>Phone Number</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.id, edit_member_path(user) %></td>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
      <td><%= user.username %></td>
      <td><%= user.address %></td>
      <td><%= user.phone_number %></td>
      <td><%= button_to "Delete", member_path(user), method: :delete %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<div>
  <%= link_to "Create User", new_member_path %>
</div>






